<!DOCTYPE html>
<html lang="en">
<head>
<script>
<div id="counter">
        <!-- counts -->
    </div>
  
    <script id="pokingwinnie">
        let counts=setInterval(updated);
        let upto=0;
        function updated(){
            var count= document.getElementById("counter");
            count.innerHTML=++upto;
            if(upto===1000)
            {
                clearInterval(counts);
            }
        }
    </script>
    </head>
